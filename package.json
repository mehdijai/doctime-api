{
    "name": "doctime-api",
    "author": "Mehdi Jai",
    "private": true,
    "license": "MIT",
    "version": "1.0.0",
    "description": "DocTime API",
    "keywords": [
        "NodeJS",
        "ExpressJS",
        "Jest",
        "Supertest",
        "Node API",
        "Medical"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/mehdijai/doctime-api"
    },
    "homepage": "https://github.com/mehdijai/doctime-api",
    "main": "./src/server.ts",
    "scripts": {
        "dev": "nodemon -r tsconfig-paths/register src/server.ts",
        "start": "node -r tsconfig-paths/register dist/server.js",
        "test": "cross-env STAGE=TEST jest --detectOpenHandles",
        "format": "prettier --write .",
        "lint": "eslint .",
        "typecheck": "tsc --noEmit -p tsconfig.json",
        "build": "npm run typecheck && rimraf dist/ && tsc && tsc-alias && node .scripts/cp.js src/templates dist/",
        "migrate:test": "ts-node ./prisma/scripts/migrate.ts",
        "prepare": "husky init"
    },
    "lint-staged": {
        "*.ts": [
            "npm run lint",
            "tsc-files --noEmit",
            "npm run format",
            "git add ."
        ]
    },
    "dependencies": {
        "@prisma/client": "^5.14.0",
        "bcrypt": "^5.1.1",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.2",
        "cheerio": "^1.0.0-rc.12",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-rate-limit": "^7.2.0",
        "handlebars": "^4.7.8",
        "helmet": "^7.1.0",
        "inline-css": "^4.0.2",
        "jsdom": "^24.1.1",
        "jsonwebtoken": "^9.0.2",
        "moment": "^2.30.1",
        "nodemailer": "^6.9.14",
        "prisma": "^5.14.0",
        "supertest": "^7.0.0",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.0",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "uuid": "^9.0.1",
        "winston": "^3.13.1",
        "x-xss-protection": "^2.0.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/body-parser": "^1.19.5",
        "@types/cookie-parser": "^1.4.7",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/inline-css": "^3.0.3",
        "@types/jest": "^29.5.12",
        "@types/jsdom": "^21.1.7",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/node": "^20.12.12",
        "@types/nodemailer": "^6.4.15",
        "@types/supertest": "^6.0.2",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.6",
        "@types/uuid": "^9.0.8",
        "@typescript-eslint/eslint-plugin": "^7.18.0",
        "@typescript-eslint/parser": "^7.18.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "husky": "^9.1.4",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.7",
        "nodemailer-mock": "^2.0.6",
        "nodemon": "^3.1.1",
        "prettier": "^3.3.3",
        "rimraf": "^6.0.1",
        "ts-jest": "^29.2.3",
        "tsc-alias": "^1.8.10",
        "tsc-files": "^1.1.4",
        "typescript": "^5.4.5"
    }
}
